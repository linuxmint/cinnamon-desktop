#: ../libgnome-desktop/gnome-wall-clock.c:260
msgid "%a %l:%M %p"
msgstr "%a %l:%M %p"

#. Translators: This is a time format without date used
#. for AM/PM.
#: ../libgnome-desktop/gnome-wall-clock.c:264
msgid "%l:%M:%S %p"
msgstr "%l:%M:%S %p"

#: ../libgnome-desktop/gnome-wall-clock.c:265
msgid "%l:%M %p"
msgstr "%l:%M %p"

